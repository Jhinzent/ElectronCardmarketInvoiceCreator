{
  "manifest_version": 2,
  "name": "Print HTML to Console",
  "version": "1.0",
  "description": "Prints the entire HTML of the current page to the console.",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Print HTML"
  }
}